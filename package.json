    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.16.1",
    "lint-staged": "^13.2.3",
    "mini-types": "^0.1.7",
    "miniprogram-api-typings": "^3.12.0",
    "picocolors": "^1.0.0",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
